   "source": [
    "import zipfile\n",
    "# 原压缩文件在服务器的位置\n",
    "# 这里的第二个参数用r表示是读取zip文件，w或a是创建一个zip文件\n",
    "f = zipfile.ZipFile(\"./detection/PennFudanPed.zip\",'r') \n",
    "# f.namelist() 会返回压缩包内所有文件名的列表\n",
    "for file in f.namelist():\n",
    "  f.extract(file,\"./\") #解压到的位置，./表示当前目录(与此.ipynb文件同一个目录)\n",
    "f.close()"
   ]
